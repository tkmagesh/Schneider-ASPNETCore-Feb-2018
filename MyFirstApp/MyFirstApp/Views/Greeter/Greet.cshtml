@model MyFirstApp.Models.UserDetails

<h1>Greeter</h1>
<hr/>
<form asp-controller="Greeter" asp-action="Greet">
    <label> First Name : </label>
    @Html.TextBoxFor(m => m.FirstName)
    <br/>
    <label> Last Name : </label>
    <input type="text" asp-for="LastName"/>
    <br/>
    <ol>
    @for(var i=0;i < 10; i++){
        <li>@i</li>
    }
    </ol>
    <input type="submit" />
</form>
<hr/>
<div>@Model.Message</div>
